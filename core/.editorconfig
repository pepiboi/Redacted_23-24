# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[*.{yaml, yml}]
indent_size = 2

[*.cs]
continuation_indent_size = 4
ij_continuation_indent_size = 4
dotnet_diagnostic.IDE0005.severity = warning
